{
  "name": "@go2asia/sdk",
  "version": "0.1.0",
  "private": true,
  "description": "Auto-generated API client SDK from OpenAPI specifications",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./atlas": {
      "types": "./src/atlas.ts",
      "default": "./src/atlas.ts"
    },
    "./blog": {
      "types": "./src/blog.ts",
      "default": "./src/blog.ts"
    },
    "./pulse": {
      "types": "./src/pulse.ts",
      "default": "./src/pulse.ts"
    },
    "./content": {
      "types": "./src/content.ts",
      "default": "./src/content.ts"
    },
    "./balance": {
      "types": "./src/balance.ts",
      "default": "./src/balance.ts"
    },
    "./transactions": {
      "types": "./src/transactions.ts",
      "default": "./src/transactions.ts"
    },
    "./referrals": {
      "types": "./src/referrals.ts",
      "default": "./src/referrals.ts"
    },
    "./clerk-integration": {
      "types": "./src/clerk-integration.ts",
      "default": "./src/clerk-integration.ts"
    },
    "./mutator": {
      "types": "./src/mutator.ts",
      "default": "./src/mutator.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "gen": "orval --config ../../orval.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@go2asia/types": "workspace:*",
    "@tanstack/react-query": "^5.90.12"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "orval": "^7.3.0",
    "typescript": "^5.3.3"
  }
}

